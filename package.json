{
  "name": "typeflow",
  "version": "0.1.0",
  "description": "Developer Typing Trainer - A lightweight, widget-first typing practice application for developers",
  "type": "module",
  "private": true,
  "license": "AGPL-3.0",
  "author": "Akimasa Sugai",
  "repository": {
    "type": "git",
    "url": "https://github.com/otomatty/typeflow.git"
  },
  "bugs": {
    "url": "https://github.com/otomatty/typeflow/issues"
  },
  "homepage": "https://github.com/otomatty/typeflow#readme",
  "keywords": [
    "typing",
    "typing-practice",
    "typing-trainer",
    "japanese",
    "romaji",
    "developer-tools",
    "react",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "dev:all": "bun run scripts/dev.ts",
    "build": "vite build",
    "typecheck": "tsc -b",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\" \"*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\" \"*.{ts,tsx,json,md}\"",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "optimize": "vite optimize",
    "preview": "vite preview",
    "server": "bun run src/server/server.ts",
    "server:dev": "bun --watch src/server/server.ts",
    "dev:server": "bun --watch src/server/server.ts",
    "db:migrate": "bun run scripts/migrate.ts",
    "db:migrate:local": "bun run scripts/migrate.ts --local",
    "db:setup": "bun run scripts/setup-local-db.ts",
    "turso:create": "turso db create typeflow-db",
    "turso:list": "turso db list",
    "turso:shell": "turso db shell typeflow-db",
    "deploy": "bunx wrangler deploy",
    "deploy:dev": "bunx wrangler deploy --env development",
    "deploy:pages": "bun run build && bunx wrangler pages deploy dist --project-name=typeflow",
    "prepare": "husky install"
  },
  "dependencies": {
    "@github/spark": ">=0.43.1 <1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^4.1.3",
    "@libsql/client": "^0.15.15",
    "@octokit/core": "^6.1.4",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.90.12",
