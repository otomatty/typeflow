    "hono": "^4.0.0",
    "i18next": "^25.7.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.484.0",
    "marked": "^17.0.1",
    "next-themes": "^0.4.6",
