{
  "name": "typeflow",
  "version": "0.1.0",
  "description": "Developer Typing Trainer - A lightweight, widget-first typing practice application for developers",
  "type": "module",
  "private": true,
  "license": "AGPL-3.0",
  "author": "Akimasa Sugai",
  "repository": {
    "type": "git",
    "url": "https://github.com/otomatty/typeflow.git"
  },
  "bugs": {
    "url": "https://github.com/otomatty/typeflow/issues"
  },
  "homepage": "https://github.com/otomatty/typeflow#readme",
  "keywords": [
    "typing",
    "typing-practice",
    "typing-trainer",
    "japanese",
    "romaji",
    "developer-tools",
    "react",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "dev:all": "bun run scripts/dev.ts",
    "build": "vite build",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:icon": "tauri icon",
    "typecheck": "tsc -b",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\" \"*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\" \"*.{ts,tsx,json,md}\"",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "optimize": "vite optimize",
    "preview": "vite preview",
    "server": "bun run src/server/server.ts",
    "server:dev": "bun --watch src/server/server.ts",
    "dev:server": "bun --watch src/server/server.ts",
    "db:migrate": "bun run scripts/migrate.ts",
    "db:migrate:local": "bun run scripts/migrate.ts --local",
    "db:setup": "bun run scripts/setup-local-db.ts",
    "turso:create": "turso db create typeflow-db",
    "turso:list": "turso db list",
    "turso:shell": "turso db shell typeflow-db",
    "preset:find": "bun run scripts/preset-manager.ts find",
    "preset:find:local": "bun run scripts/preset-manager.ts find --local",
    "preset:fix": "bun run scripts/preset-manager.ts fix",
    "preset:fix:local": "bun run scripts/preset-manager.ts fix --local",
    "preset:export": "bun run scripts/preset-manager.ts export",
    "preset:export:local": "bun run scripts/preset-manager.ts export --local",
    "preset:import": "bun run scripts/preset-manager.ts import",
    "preset:import:local": "bun run scripts/preset-manager.ts import --local",
    "cli:practice": "bun run scripts/cli-practice.ts",
    "cli:practice:log": "bun run scripts/cli-practice.ts --mode=log",
    "cli:practice:test": "bun run scripts/cli-practice.ts --mode=test",
    "cli:practice:build": "bun run scripts/cli-practice.ts --mode=build",
    "cli:practice:git": "bun run scripts/cli-practice.ts --mode=git",
    "deploy": "bunx wrangler deploy",
    "deploy:dev": "bunx wrangler deploy --env development",
    "deploy:pages": "bun run build && bunx wrangler pages deploy dist --project-name=typeflow",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/backend": "^1.0.0",
